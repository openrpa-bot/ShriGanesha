{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/addtoany": "2.0.4",
        "drupal/admin_toolbar": "3.4.1",
        "drupal/antibot": "2.0.2",
        "drupal/asset_injector": "2.17",
        "drupal/autocomplete_deluxe": "2.0.3",
        "drupal/background_image": "2.0.1",
        "drupal/barcodes": "2.0.5",
        "drupal/better_exposed_filters": "6.0.3",
        "drupal/better_social_sharing_buttons": "4.0.4",
        "drupal/bootstrap5": "3.0.8",
        "drupal/bootstrap_barrio": "5.5.12",
        "drupal/bootstrap_layout_builder": "2.1.1",
        "drupal/bpmn_io": "1.1.1",
        "drupal/captcha": "2.0.0",
        "drupal/clientside_validation": "4.0.2",
        "drupal/commerce": "2.36",
        "drupal/commerce_add_to_cart_link": "2.0.5",
        "drupal/commerce_license": "3.0.0-rc6",
        "drupal/commerce_shipping": "2.6",
        "drupal/conditional_fields": "4.0.0-alpha3",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/country": "1.1",
        "drupal/ctools": "4.0.4",
        "drupal/devel": "5.1.2",
        "drupal/drupal_commerce_razorpay": "1.0.0",
        "drupal/dxpr_builder": "2.2.6",
        "drupal/dxpr_theme": "5.1.0",
        "drupal/eca": "1.1.3",
        "drupal/eca_cm": "1.0.5",
        "drupal/eca_state_machine": "1.0.1",
        "drupal/eca_tamper": "1.0",
        "drupal/editor_file": "1.7",
        "drupal/entity_class_formatter": "2.0.0",
        "drupal/entity_embed": "1.4",
        "drupal/entity_field_condition": "1.4",
        "drupal/exclude_node_title": "1.4",
        "drupal/extlink": "1.7",
        "drupal/extra_field": "2.3",
        "drupal/facets": "2.0",
        "drupal/field_formatter_class": "1.6",
        "drupal/field_group": "3.4",
        "drupal/field_permissions": "1.2",
        "drupal/field_token_value": "3.0.1",
        "drupal/flood_control": "2.3.2",
        "drupal/geocoder": "4.9",
        "drupal/geofield": "1.53",
        "drupal/geofield_map": "3.0.10",
        "drupal/geolocation": "3.12",
        "drupal/google_analytics": "4.0.2",
        "drupal/google_map_field": "2.0.0",
        "drupal/google_tag": "2.0.2",
        "drupal/jquery_ui_checkboxradio": "2.0.0",
        "drupal/jquery_ui_tooltip": "2.0.0",
        "drupal/layout_builder_modal": "1.2",
        "drupal/layout_builder_restrictions": "2.17",
        "drupal/menu_per_role": "1.5",
        "drupal/metatag": "1.25",
        "drupal/modal_page": "5.0.1",
        "drupal/multiple_registration": "3.2.0",
        "drupal/multiple_selects": "1.1",
        "drupal/page_manager": "4.0-rc2",
        "drupal/pathauto": "1.11",
        "drupal/pdf_api": "2.3.1",
        "drupal/poll": "1.6",
        "drupal/prlp": "1.10",
        "drupal/r4032login": "2.2.1",
        "drupal/recaptcha": "3.2",
        "drupal/search_api": "1.29",
        "drupal/search_api_autocomplete": "1.7",
        "drupal/simple_popup_blocks": "3.1",
        "drupal/smtp": "1.2",
        "drupal/social_auth": "4.0.1",
        "drupal/social_auth_facebook": "4.0.1",
        "drupal/social_auth_google": "4.0.0",
        "drupal/social_media_links": "2.9",
        "drupal/tablefield": "2.4",
        "drupal/tamper": "1.0.0-alpha4",
        "drupal/text_field_formatter": "2.0.1",
        "drupal/twig_debugger": "1.1.3",
        "drupal/twig_field_value": "2.0.2",
        "drupal/uuid_extra": "2.0.1",
        "drupal/view_modes_display": "3.0.0",
        "drupal/views_conditional": "1.5",
        "drupal/views_slideshow": "5.0.0",
        "drupal/webform": "6.2.0-beta6@beta",
        "drupal/webform_content_creator": "4.0.4",
        "drupal/webform_gmap_field": "1.1",
        "drupal/webform_ip_geo": "1.0.4",
        "drupal/webform_product": "3.0.4",
        "drupal/webform_views": "5.1",
        "drupal/webprofiler": "10.0.0",
        "drupal/workflow": "1.7",
        "drupal/xmlsitemap": "1.4",
        "drupal/youtube": "2.0.0",
        "drush/drush": "12.1.0",
        "sibyx/phpgpx": "@RC"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
