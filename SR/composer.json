{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/addtoany": "2.0.1",
        "drupal/admin_toolbar": "3.3",
        "drupal/advancedqueue": "1.0-RC7",
        "drupal/antibot": "2.0.2",
        "drupal/autocomplete_deluxe": "2.0.3",
        "drupal/bat": "1.3.0",
        "drupal/bat_api": "1.1.0",
        "drupal/better_exposed_filters": "6.0.3",
        "drupal/better_social_sharing_buttons": "4.0.3",
        "drupal/bootstrap5": "3.0.5",
        "drupal/bootstrap_barrio": "5.5.10",
        "drupal/bootstrap_layout_builder": "2.1.1",
        "drupal/captcha": "1.10",
        "drupal/clientside_validation": "4.0.2",
        "drupal/commerce": "2.34",
        "drupal/commerce_add_to_cart_link": "2.0.5",
        "drupal/commerce_donate": "1.1.0-alpha1",
        "drupal/commerce_license": "2.0.0-beta2",
        "drupal/commerce_shipping": "2.6",
        "drupal/commerce_stock": "1.0",
        "drupal/commerce_ticketing": "2.0.0-alpha7",
        "drupal/commerce_webform_order": "2.0.0-beta2",
        "drupal/conditional_fields": "4.0.0-alpha2",
        "drupal/core-composer-scaffold": "^9.5",
        "drupal/core-project-message": "^9.5",
        "drupal/core-recommended": "^9.5",
        "drupal/country": "1.1",
        "drupal/dbug": "2.0.0",
        "drupal/devel": "5.1.1",
        "drupal/editor_file": "1.7",
        "drupal/exclude_node_title": "1.4",
        "drupal/externalauth": "2.0.3",
        "drupal/extlink": "1.7",
        "drupal/extra_field": "2.3",
        "drupal/facets": "2.0",
        "drupal/field_formatter_class": "1.6",
        "drupal/field_group": "3.4",
        "drupal/field_permissions": "1.2",
        "drupal/flexslider": "2.0",
        "drupal/flood_control": "2.3.2",
        "drupal/fullcalendar_library": "1.1",
        "drupal/geocoder": "3.32",
        "drupal/geofield": "1.52",
        "drupal/geofield_map": "3.0.8",
        "drupal/geolocation": "3.12",
        "drupal/google_analytics": "4.0.2",
        "drupal/google_analytics_reports": "3.0",
        "drupal/google_map_field": "2.0.0",
        "drupal/google_tag": "1.6",
        "drupal/image_resize_filter": "1.1",
        "drupal/inline_entity_form": "1.0-rc15",
        "drupal/jquery_ui_checkboxradio": "2.0.0",
        "drupal/layout_builder_modal": "1.2",
        "drupal/layout_builder_restrictions": "2.17",
        "drupal/libraries": "4.0.1",
        "drupal/linkchecker": "1.1",
        "drupal/menu_per_role": "1.5",
        "drupal/metatag": "1.22",
        "drupal/openid_connect": "1.2",
        "drupal/page_manager": "4.0-rc2",
        "drupal/pathauto": "1.11",
        "drupal/pdf_api": "2.3.0",
        "drupal/poll": "1.6",
        "drupal/prlp": "1.10",
        "drupal/r4032login": "2.2.1",
        "drupal/recaptcha": "3.1",
        "drupal/recaptcha_v3": "1.8",
        "drupal/redirect_after_login": "2.7",
        "drupal/s3fs": "3.1",
        "drupal/search_api": "1.29",
        "drupal/search_api_autocomplete": "1.7",
        "drupal/search_api_location": "1.0-alpha3",
        "drupal/sendgrid_integration": "2.1",
        "drupal/seo_checklist": "5.1.0",
        "drupal/services": "dev-4.x",
        "drupal/simple_popup_blocks": "3.1",
        "drupal/social_auth": "3.1.0",
        "drupal/social_auth_google": "3.0.0",
        "drupal/social_media_links": "2.9",
        "drupal/tablefield": "2.4",
        "drupal/tb_megamenu": "1.7",
        "drupal/tfa": "1.0",
        "drupal/twig_field_value": "2.0.0",
        "drupal/user_email_verification": "1.1",
        "drupal/views_conditional": "1.5",
        "drupal/views_slideshow": "5.0.0",
        "drupal/webform": "6.2.0-beta5",
        "drupal/webform_ip_geo": "1.0.4",
        "drupal/xmlsitemap": "1.4",
        "drupal/youtube": "2.0.0",
        "drush/drush": "11.5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
